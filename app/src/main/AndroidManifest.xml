<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tomward.worldbook">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorldBook">
        android:usesCleartextTraffic="true">
        <activity android:name=".WorldView.EnterCountryName"></activity>
        <activity android:name=".CountyInfo.PicturesActivity" />
        <activity android:name=".CountyInfo.CountryActivity" />
        <activity android:name=".CountyInfo.AddPictureActivity" />
        <activity android:name=".WorldView.GlobeManager"/>
        <activity android:name=".RegisterActivity" />

        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>